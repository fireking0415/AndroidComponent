<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.fireking.login">

    <application
        android:name="org.fireking.commons.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/login_login_ic_launcher"
        android:label="@string/login_app_name"
        android:roundIcon="@mipmap/login_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/login_AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="org.fireking.login.LoginAppDelegate"
            android:value="ConfigModule" />
    </application>

</manifest>